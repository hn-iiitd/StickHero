<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="379.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iiitd.stickhero.ReviveController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="482.0" layoutX="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../back2%20(1).jpg" />
         </image>
      </ImageView>
      <Label layoutX="125.0" layoutY="197.0" text="DO YOU WANT TO REVIVE?">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="181.0" layoutY="262.0" mnemonicParsing="false" onAction="#switchToNewGame" text="YES" />
      <Button layoutX="258.0" layoutY="262.0" mnemonicParsing="false" onAction="#switchToGameOver" text="NO" />
      <Label fx:id="error" layoutX="152.0" layoutY="321.0" prefHeight="22.0" prefWidth="194.0" textFill="#d92323" />
   </children>
</AnchorPane>
